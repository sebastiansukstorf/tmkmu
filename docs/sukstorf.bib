@thesis{subow2019,
  title = {Mitarbeiterbindung in Klein- und Mittelunternehmen – Kann Talent Management diese fördern?},
  author = {Subow, Ina},
  date = {2019-02-01},
  institution = {{Hochschulbibliothek HWR Berlin}},
  url = {https://opus4.kobv.de/opus4-hwr/frontdoor/index/index/docId/1464},
  urldate = {2021-03-08},
  abstract = {Die vorliegende Bachelorarbeit gibt einen Überblick über Mitarbeiterbindung und Talent Management in Klein- und Mittelunternehmen (KMU). Ziel der Arbeit ist es empirisch zu untersuchen, ob Talent Management eine geeignete Maßnahme zur Personalbindung in KMU ist. Des Weiteren wird untersucht, ob und wenn ja, welche Unterschiede sich hinsichtlich der Ausprägung der Strategie, Verantwortung und Zielsetzung des Talent Managements in den Unternehmen, basierend auf der Größe, feststellen lassen. Hierfür werden deutschlandweit kleine und mittlere Unternehmen mit Hilfe eines online Fragebogens befragt. Die wichtigste Erkenntnis aus der anonymen Umfrage ist, dass Talent Management, unabhängig von der Unternehmensgröße, als wirkungsvolle Maßnahme zur Personalbindung angesehen wird. Weiterhin konnten die Erkenntnisse aus der Literaturrecherche bestätigt werden, dass mit steigender Beschäftigtenanzahl die Umsetzung des Talent Managements strategischer und professioneller erfolgt. Es ließ sich zudem feststellen, dass kaum Unterschiede hinsichtlich der genutzten Maßnahmen zur Mitarbeiterbindung vorhanden sind und diese größtenteils mit dem „Lebenszyklus“ des Mitarbeiters übereinstimmen. Die Bachelorarbeit ist sowohl für Inhaber/Geschäftsführer eines KMU als auch für alle personalverantwortlichen Mitarbeiter in diesen interessant.},
  annotation = {00000  ZSCC: 0000000},
  file = {/Users/sebastian.sukstorf/Zotero/storage/SJ8Y4CFH/1464.html},
  langid = {deu}
}

